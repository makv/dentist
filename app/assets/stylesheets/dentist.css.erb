}

.slide .content {
    background: none repeat scroll 0 0 transparent;
    color: #FFFFFF;
    display: block;
    font-family: 'Helvetica Neue';
    height: 250px;
    left: 510px;
    padding: 0.2em 10px;
    position: absolute;
    top: 5px;
    width: 500px;
    z-index: 1000;
}

.slides .content h2 {
    color: white;
    font-family: 'Helvetica Neue';
    font-size: 1.8em;
    font-weight: normal;
    margin-top: 10px;
    text-transform: none;
