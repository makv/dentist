}

.slideShow ul, .slideShow li {
    list-style-type: none;
    margin: 0;
    padding: 0;
}

.slideShow {
    clear: both;
    display: block;
    height: 317px;
}

.slideShow .navigation {
    clear: both;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: -32px;
    padding-left: 10px;
    position: relative;
    width: 950px;
    z-index: 99 !important;
}

.slideShow .navigation li {
    display: inline;
}

.slideShow .navigation a {
    -moz-transition: all 0.2s ease 0s;
    background-color: white;
    border: 1px solid #3C6EB4;
    border-radius: 3px 3px 3px 3px;
    color: #3C6EB4;
    display: inline-block;
    font-size: 0.8em;
    font-weight: bold;
    margin: 0 2px 0 0;
    padding: 3px 8px;
    text-decoration: none;
}

.slideShow .navigation a:hover, .slideShow .navigation a.selected {
    -moz-transition: all 0.2s ease 0s;
    background-color: #3C6EB4;
    border: 1px solid white;
    color: white;
    font-weight: bold;
}

.simpleSlideShow {
    padding-bottom: 2em;
    width: 100%;
}

ul.slides {
    width: 100%;
}

li.slide {
    width: 100% !important;
}

.slide-canvas {
    display: block;
    height: 317px !important;
    margin-left: auto;
    margin-right: auto;
    position: relative;
    width: 960px !important;
    background: none repeat scroll 0 0 transparent;
}

.slide-canvas div {
    display: block;
}

.slides #dentist-slide {
    background-color: #444444;
}

.slides li p {
