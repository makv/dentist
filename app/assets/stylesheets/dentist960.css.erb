ul.tabs {
    height: 30px;
}
ul.tabs li {
    float: left;
    list-style-type: none;
    margin: 0;
    padding: 0;
}
ul.tabs a {
    background-color: #DCDCDC;
    color: #3771C8;
    display: block;
    font-weight: bold;
    height: 18px;
    padding: 5px 10px;
    text-decoration: none;
}
ul.tabs a:hover {
    background-color: #FFFFFF;
    color: #818181;
    font-weight: bold;
}
ul.tabs a.current {
    background-color: #FFFFFF;
    color: #808080;
    cursor: default;
    font-weight: bold;
}

.slideShow ul, .slideShow li {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
.slideShow {
    clear: both;
    display: block;
    height: 317px;
}
.slideShow .navigation {
    clear: both;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: -32px;
    padding-left: 10px;
    position: relative;
    width: 950px;
    z-index: 99 !important;
}
.slideShow .navigation li {
    display: inline;
}
.slideShow .navigation a {
    -moz-transition: all 0.2s ease 0s;
    background-color: white;
    border: 1px solid #3C6EB4;
    border-radius: 3px 3px 3px 3px;
    color: #3C6EB4;
    display: inline-block;
    font-size: 0.8em;
    font-weight: bold;
    margin: 0 2px 0 0;
    padding: 3px 8px;
    text-decoration: none;
}
.slideShow .navigation a:hover, .slideShow .navigation a.selected {
    -moz-transition: all 0.2s ease 0s;
    background-color: #3C6EB4;
    border: 1px solid white;
    color: white;
    font-weight: bold;
}
.simpleSlideShow {
	padding-bottom: 2em;
    width: 100%;
}
ul.slides {
    width: 100%;
}
li.slide {
    width: 100% !important;
}
.slide-canvas {
    display: block;
    height: 317px !important;
    margin-left: auto;
    margin-right: auto;
    position: relative;
    width: 960px !important;
}
.slide-canvas div {
    display: block;
}
.slides #dentist-slide {
    background-color: #444444;
}
.slides li p {
    font-size: 1.1em;
}
.slide div a {
    color: white;
}
.slide .content {
    background: none repeat scroll 0 0 transparent;
    color: #FFFFFF;
    display: block;
    font-family: "CantarellOblique","Droid Sans",Verdana,sans-serif;
    height: 250px;
    left: 510px;
    padding: 0.2em 10px;
    position: absolute;
    top: 5px;
    width: 500px;
    z-index: 1000;
}
.slides .content h2 {
    color: white;
    font-family: "CantarellOblique","Droid Sans",Verdana,sans-serif;
    font-size: 1.8em;
    font-weight: normal;
    margin-top: 10px;
    text-transform: none;
}
.slides #dentist-slide div h2 {
    font-weight: bold;
    letter-spacing: -2px;
    text-shadow: 1px 1px 5px #3C6EB4;
}
.slides #dentist-slide a {
    color: #255591;
}
.slides #dentist-slide div {
    color: #444444;
    font-size: 1.1em;
}
.slides #dentist-slide li a {
    background: -moz-linear-gradient(center bottom , #A9C8DA, #FFFFFF) repeat scroll 0 0 transparent;
    border-bottom: 2px solid #255591;
    border-radius: 8px 8px 8px 8px;
    color: #255591;
    float: right;
    font-family: "ComfortaaRegular",sans-serif;
    font-size: 1.1em;
    margin-right: 0.5em;
    padding: 0.25em 0.65em;
    text-shadow: 1px 1px 0 white;
}
.slides #dentist-slide p.small {
    clear: right;
    color: #444444;
    font-size: 10pt;
    margin-right: 1em;
    padding-top: 0.6em;
    text-align: right;
}
.slides #dentist-slide li a:hover {
    background: -moz-linear-gradient(center top , #A9C8DA, #FFFFFF) repeat scroll 0 0 transparent;
    color: #255591;
    text-shadow: -1px -1px 0 white;
}
.slides #dentist-slide {
    background: url(<%= asset_path 'slides-bg.gif' %>) repeat scroll center top white;
}
.slides #dentist-slide .content, .slides #dentist-slide p.small {
    color: white;
}
